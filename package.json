{
  "name": "food-ordering-system",
  "version": "1.0.0",
  "description": "A desktop food ordering system built with Electron and Python",
  "main": "main.js",
  "scripts": {
    "start": "node ./scripts/start-all.js",
    "electron-only": "electron .",
    "dev": "electron . --dev",
    "build:css": "tailwindcss -i ./input.css -o ./output.css --watch",
    "build:css:prod": "tailwindcss -i ./input.css -o ./output.css --minify"
  },
  "keywords": [
    "electron",
    "food",
    "ordering",
    "python",
    "flask"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "sweetalert2": "^11.26.3",
    "tailwindcss": "^4.1.17"
  }
}
